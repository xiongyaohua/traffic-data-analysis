    "X_model = np.linspace(-3, 3, 50)\n",
    "\n",
    "plt.plot(X, Y, \"o\")\n",
    "\n",
    "for n in range(1, 5):\n",
    "    theta = regression(X, Y, n)\n",
    "    Yhat = predict(X, theta)\n",
    "    Y_model = predict(X_model, theta)\n",
    "\n",
    "    plt.plot(X_model, Y_model, label=\"degree of {}\".format(n))\n",
    "plt.ylim(-5, 10)\n",
    "plt.title(\"Compare regression of different degrees\")\n",
    "plt.legend()\n",
    "plt.xlabel(\"X\")\n",
    "plt.ylabel(\"Y\")"
   ]
